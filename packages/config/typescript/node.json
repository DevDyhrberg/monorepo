{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "es2016",
        "lib": ["dom", "dom.iterable", "esnext"],
        "sourceMap": true,
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "incremental": true,
        "esModuleInterop": true,
        "module": "CommonJS",
        "isolatedModules": true
    }
}